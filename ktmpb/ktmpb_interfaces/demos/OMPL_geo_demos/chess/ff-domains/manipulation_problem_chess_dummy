(define (problem manip_pawnB1)

(:domain chessmanipulation_dummy)

(:objects
    pos1 pos2 - location
    pawnB1 - obstacle
    ur3a - robot
    d1 d2 - dummyparam
)

(:init
    (in pawnB1 pos1)
    (handEmpty ur3a)
    (not (dummyCalled ur3a d1 d2))
)

(:goal
    (and (in pawnB1 pos2)
         (dummyCalled ur3a d1 d2))
)
)
